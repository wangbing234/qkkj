{
  "name": "React",
  "version": "0.0.0",
  "description": "React 项目",
  "scripts": {
    "start": "node server.js",
    "FileManage": "node FileManage/server.js",
    "ConfigCenter": "node ConfigCenter/server.js",
    "DataManager": "node DataManager/server.js",
    "DataFactory": "node DataFactory/server.js",
    "SecurityCenter": "node SecurityCenter/server.js",
    "HackathonAPI": "node HackathonAPI/server.js",
    "ReactJupiterEditor": "node ReactJupiterEditor/server.js",
    "BDOS-UI": "node BDOS-UI/site/server.js",
    "buildBDOS-UI": "rm -rf BDOS-UI/site/dist && webpack --display -error-details -p -c --optimize-minimize  --config BDOS-UI/site/webpack.config.release.js",
    "buildConfigCenter": "webpack --display -error-details -p -c --optimize-minimize  --config ConfigCenter/webpack.config.release.js ",
    "buildDataManager": "webpack --display -error-details -p -c --optimize-minimize  --config DataManager/webpack.config.release.js ",
    "buildFileManage": "rm -rf FileManage/static && webpack -p --progress  --config FileManage/webpack.config.release.js ",
    "buildDataFactory": "webpack --display -error-details -p -c --optimize-minimize  --config DataFactory/webpack.config.release.js ",
    "buildSecurityCenter": "webpack --display -error-details -p -c --optimize-minimize  --config SecurityCenter/webpack.config.release.js ",
    "buildHackathonAPI": "rm -rf HackathonAPI/static && webpack --display -error-details -p -c --optimize-minimize  --config HackathonAPI/webpack.config.release.js ",
    "runAll": "npm run buildConfigCenter & npm run buildDataManager & npm run buildFileManage & npm run buildDataFactory & npm run buildSecurityCenter"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "bfd-bootstrap": "0.0.22",
    "bootstrap": "^3.3.6",
    "classnames": "^2.1.2",
    "d3": "^3.5.11",
    "dateformat": "^1.0.12",
    "echarts": "^3.2.1",
    "font-awesome": "^4.6.3",
    "history": "^1.17.0",
    "lodash": "^4.14.2",
    "react": "~0.14.8",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-pure-render-mixin": "^15.3.1",
    "react-dom": "~0.14.8",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.0",
    "react-update": "^0.3.8",
    "warning": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^5.6.18",
    "babel-loader": "^5.1.4",
    "babel-plugin-transform-runtime": "~6.4.3",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.19.0",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "express": "^4.13.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "~1.12.1",
    "extract-text-webpack-plugin": "^1.0.1"
  }
}
